/*Написать программу которая считывает 5-и значное число с клавиатуры 
и выводит цифры из которого он состоит*/

package homework;

import java.util.Scanner;

public class Home {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		System.out.print("Введите целое пятизначное число: ");
	    for(int number = in.nextInt(), i=10000; i>=1; number%=i, i/=10) 
	    	System.out.println(number / i); 
	    in.close();
	}

}
